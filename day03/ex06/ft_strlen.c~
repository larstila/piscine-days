#include <unistd.h>

void	ft_putchar(char c)
{
	write(1, &c, 1);
}

int 	strlen(char *str)
{
	int len;

	while(str[len] != '\0') 
	{
		len++
	}
	return len;
}

int main(void)
{
	char *str;

	str = "testi";
	ft_putchar(ft_strlen(str) + '0');
	return (0);
}
