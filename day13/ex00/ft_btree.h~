#ifndef FT_BTREE_H
# define FT_BTREE_H

# include <stdlib.h>
# include <unistd.h>

typedef struct		s_btree
{
	struct s_btree	*left;
	struct s_btree	*right;
	void			*item;
}					t_btree;

t_btree				*btree_create_node(void *item);
void				print_btree(t_btree *parent, int level);

#endif
