#define <unistd.h>

void    ft_putchar(char c)
{
    write(1, &c, 1);
    return ;
}

void 	ft_print_comb(void)
{

	char i; j; k;
	i = 0;
	j = 1;
	k = 2;
	while ()
{
	if (i < j < k)
	{
		ft_putchar(i);
		ft_putchar(j);
		ft_putchar(k);
		ft_putchar(,);
		ft_putchar('\n');
	}
}


	return ;
}

int 	main(void)
{
	ft_print_comb();
	return ;
}
					