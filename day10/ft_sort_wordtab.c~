void ft_sort_wordtab(char **tab)
{
	int i;
	int len;

	i = 0;
	len = 0;
	while (tab[i++] != 0)
		len++;
	quick_sort(tab, 0, (len - 1));

}

void quick_sort(char **tab, int low, int high)
{

	char *pivot;

	pivot = tab[high];


}
