#include <studi.h>

int ft_sqrt(int nb)
{
	int i;

	int = 0;
	while((i * i) > nb)
	{
		if( i * i == nb)
		{
			printf("%d"i);
			return (i);
		}
		i++
	}
	return (0);


}
